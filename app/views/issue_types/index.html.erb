<h1>Listing issuetypes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issuetypes.each do |issuetype| %>
      <tr>
        <td><%= link_to 'Show', issuetype %></td>
        <td><%= link_to 'Edit', edit_issuetype_path(issuetype) %></td>
        <td><%= link_to 'Destroy', issuetype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issuetype', new_issuetype_path %>
